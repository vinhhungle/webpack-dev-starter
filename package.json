{
  "name": "es6-angular1-express-webpack-starter",
  "version": "1.0.0",
  "description": "An Angular 1.x starter project using webpack and babel to transpile ES6 codes.",
  "main": "server/main.js",
  "scripts": {
    "start": "npm run build && node server/main.js",
    "serve": "npm run build && node_modules/.bin/serve -p 3000 dist/",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p --progress",
    "clean": "rm dist/build/*",
    "start:dev": "node_modules/.bin/npm-run-all --parallel dev:*",
    "dev:client": "node_modules/.bin/webpack-dev-server -d --compress --open",
    "dev:client:rs": "node_modules/.bin/webpack-dev-server -d --compress",
    "dev:server": "node_modules/.bin/nodemon --debug server/main.js"
  },
  "author": "vinhhung.le@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "autoprefixer": "6.6.1",
    "awesome-typescript-loader": "^3.0.8",
    "babel-loader": "6.2.10",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "imports-loader": "0.7.0",
    "json-loader": "0.5.4",
    "node-pre-gyp": "^0.6.32",
    "node-sass": "4.1.1",
    "nodemon": "1.11.0",
    "postcss-loader": "1.2.1",
    "raw-loader": "0.5.1",
    "sass-loader": "4.1.1",
    "source-map-loader": "^0.1.6",
    "strip-loader": "0.1.2",
    "style-loader": "0.13.1",
    "typescript": "^2.2.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "babel-core": "6.21.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-syntax-export-extensions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "6.18.0",
    "compression": "1.6.2",
    "express": "4.14.0",
    "npm-run-all": "4.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-templates": "^0.6.1",
    "react-templates-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "serve": "^2.1.2"
  }
}
